{
  "name": "elm-ui-hexcolor",
  "version": "1.0.1",
  "description": "TheSacredLipton",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all -p watch:*",
    "watch:cpx": "cpx \"src/**/*.elm\" examples/src --watch",
    "watch:elm": " cd ./examples && elm reactor --port 8080",
    "test": "elm-test --fuzz 100",
    "edp": "edp -b -p 8080",
    "update": "npx ncu -u && npm update"
  },
  "devDependencies": {
    "cpx2": "^3.0.0",
    "elm": "^0.19.1-3",
    "elm-doc-preview": "^5.0.3",
    "elm-test": "^0.19.1-revision2",
    "npm-run-all": "^4.1.5"
  }
}